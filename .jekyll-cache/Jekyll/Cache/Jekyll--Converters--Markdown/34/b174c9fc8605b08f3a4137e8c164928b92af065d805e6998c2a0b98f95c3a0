import numpy as np
import torch
data=[[1, 2],[3, 4]]# åˆ—è¡¨list
tensor=torch.Tensor(data) #tensor</li>
</ul>

<p>print(
    â€˜\n np.matmulâ€™, np.matmul(data,data),#çŸ©é˜µä¹˜æ³•
